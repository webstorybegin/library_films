{
  "compilerOptions": {
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "allowSyntheticDefaultImports": true,
    "baseUrl": "src",
    "paths": {
      "images": ["resources/img/*"],
      "icons": ["resources/icons/*"]
    }
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "src/**/*.svg", "src/custom.d.ts"],
  "exclude": ["node_modules"]
}
